<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>registration</servlet-name>
    <servlet-class>app.servlets.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>registration</servlet-name>
    <url-pattern>/registration</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>authentication</servlet-name>
    <servlet-class>app.servlets.AuthenticationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>authentication</servlet-name>
    <url-pattern>/authentication</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>bookingHall</servlet-name>
    <servlet-class>app.servlets.BookHallServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>bookingHall</servlet-name>
    <url-pattern>/bookingDto/bookHall</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>bookingDesk</servlet-name>
    <servlet-class>app.servlets.BookDeskServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>bookingDesk</servlet-name>
    <url-pattern>/bookingDto/bookDesk</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>bookingManager</servlet-name>
    <servlet-class>app.servlets.BookingManagementServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>bookingManager</servlet-name>
    <url-pattern>/bookingDto/manager</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>coworkingSpaceHalls</servlet-name>
    <servlet-class>app.servlets.HallsManagementServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>coworkingSpaceHalls</servlet-name>
    <url-pattern>/coworking/halls</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>coworkingSpaceRooms</servlet-name>
    <servlet-class>app.servlets.RoomsManagementServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>coworkingSpaceRooms</servlet-name>
    <url-pattern>/coworking/rooms</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>coworkingSpaceDesks</servlet-name>
    <servlet-class>app.servlets.DesksManagementServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>coworkingSpaceDesks</servlet-name>
    <url-pattern>/coworking/desks</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>allBookingsByDate</servlet-name>
    <servlet-class>app.servlets.GetAllBookingsByDateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>allBookingsByDate</servlet-name>
    <url-pattern>/bookingDto/bydate</url-pattern>
  </servlet-mapping>


</web-app>
